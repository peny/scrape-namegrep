{
  "name": "namegrep-scraper",
  "version": "1.0.0",
  "description": "A Node.js scraper for namegrep.com using headless browser",
  "main": "check-browser.js",
  "homepage": "https://github.com/peny/scrape-namegrep",
  "scripts": {
    "start": "node check-browser.js",
    "start:api": "node api-only.js",
    "start:frontend": "npx serve public -l 3000",
    "build": "npm install",
    "heroku-postbuild": "npm run install-browsers",
    "test": "node scraper-backup.js",
    "api": "node api.js",
    "dev": "node api.js",
    "scraper": "node scraper-backup.js",
    "postinstall": "npx playwright install chromium",
    "install-browsers": "npx playwright install chromium"
  },
  "keywords": ["scraper", "puppeteer", "namegrep", "domains"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.40.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
