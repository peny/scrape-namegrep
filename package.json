{
  "name": "namegrep-scraper",
  "version": "1.0.0",
  "description": "A Node.js scraper for namegrep.com with Memphis design frontend",
  "main": "api.js",
  "homepage": "https://github.com/peny/scrape-namegrep",
  "scripts": {
    "start": "node api.js",
    "dev": "node api.js",
    "api": "node api.js",
    "start:frontend": "npx serve public -l 3000",
    "test": "node scraper-backup.js",
    "scraper": "node scraper-backup.js",
    "build": "npm install && npm run install-browsers",
    "install-browsers": "npx playwright install chromium",
    "postinstall": "echo 'Installing Playwright browsers...' && npx playwright install chromium && echo 'Browser installation completed!'"
  },
  "keywords": ["scraper", "playwright", "namegrep", "domains", "memphis-design", "regex"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "playwright": "^1.40.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "serve": "^14.2.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
